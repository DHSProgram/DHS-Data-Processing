          ES: |
            
          FR: |
            
  - name: CCIN80.AECD13
    conditions:
      - questionText:
          EN: |
            <p><span style="font-size: 21px;">ECD13.</span><span style="font-size: 21px;">&nbsp;</span><font face="Arial, sans-serif"><span style="font-size: 21px;">If you ask ~~AECDCN~~ to give you 3 objects, such as 3 stones or 3 beans, does (he/she) give you the correct amount?</span></font><br></p>
          ES: |
            
