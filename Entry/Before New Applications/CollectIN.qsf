  - name: CCIN80.A1031
    conditions:
      - questionText:
          EN: |
            <p><span style="font-size:21px;color:#000000;font-family:'Arial', sans-serif;">1031. How many times have you been tested for HIV in your lifetime?&#160;</span></p>
