          ES: |
            
          FR: |
            
  - name: CCIN80.AECD12
    conditions:
      - questionText:
          EN: |
            <p><span style="font-size: 21px;">ECD12.</span><span style="font-size: 21px;">&nbsp;</span><font face="Arial, sans-serif"><span style="font-size: 21px;">Can ~~AECDCN~~ recognize all numbers from 1 to 5?</span></font><br></p>
          ES: |
            
          FR: |
            
