f1w columns  fert = 0 nodec
             fert = 1 nodec;

amfw columns mort = 0 nodec
             mort = 1 nodec;

ammw columns mort = 0 nodec
             mort = 1 nodec;

mmtw columns mort = 0 nodec
             mort = 1 nodec;

pmtw columns mort = 0 nodec
             mort = 1 nodec;
{
f1w  noprint;
ammw noprint;
amfw noprint;
mmtw noprint;
pmtw noprint;
agedis noprint;
}
