        1:2  -> 1;
        3:5  -> 2;
             -> 9;
  endrecode;
  { living children }
  recode  V218  -> v218w;
            0   -> 0;
           1:2  -> 1;
           3:4  -> 2;
                -> 3;
  endrecode;
  { employed last 12 months }
  recode  V741  -> v741w1;
        notappl -> 0;
          1:2   -> 1;
          0,3   -> 2;
                -> 9;
  endrecode;
  { age }
  recode  V012  -> age5;
          15:19 -> 1;
          20:34 -> 2;
                -> 3;
  endrecode;


{ --------------------------------------------------------------------------- }
{ Tables 9.1 }

  livestil = 1;
  for i in REC41_EDT do
    if M80 in 1,3 & P19(MIDXP) < 24 then  // last birth/stillbirth in the last 2 years
      border2 = notappl;
      if M80 in 1,2 then
        border2 = RecBorder( MIDX );       { birth order }
	  endif;
      age3    = RecAge( MIDXP );           { mother's age at birth }
	  livbirth = ( M80 = 1 );
	  stlbirth = ( M80 = 3 );

      ancprov = notappl;
      if     M2A = 1                     then ancprov = 1    { doctor }
      elseif M2B = 1                     then ancprov = 2    { nurse/midwife }
      elseif M2C = 1                     then ancprov = 3    { auxiliary nurse/midwife }
      elseif M2H = 1                     then ancprov = 4    { community health worker }
      elseif M2D = 1 | M2E = 1 | M2F = 1 then ancprov = 5    { other health worker }
      elseif M2G = 1                     then ancprov = 6    { include trained and traditional birth attendant }
      elseif M2I = 1 | M2J = 1 | M2K = 1 |
             M2L = 1 | M2M = 1           then ancprov = 7    { other, not health professional }
      elseif M2A = missing               then ancprov = 9    { missing }
      endif;
      noanc = ( M2N = 1 );                                   { no one }
      xtab( t901, rweight );
      xtab( t901u );
      livestil = 0;
    endif;
  enddo;

