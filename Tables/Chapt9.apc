  { function to recode birth order as it is used by several tables }
  function RecBorder( chi )
    xtemp = BORD(chi);
    { adjust multiple births to give the same order as that of the first of the multiples }
    if B0(chi) > 1 then xtemp = BORD(chi) - B0(chi) + 1 endif;
    recode xtemp -> x;
              1  -> 0;
            2:3  -> 1;
            4:5  -> 2;
                 -> 3;
    endrecode;
    RecBorder = x;
  end;

  { function to recode mother's age as it is used by several tables }
  function RecAge( chi )
    age = int( (P3(chi)-V011)/12 );
    recode  age  -> x;
          <20 -> 0;
        20:34 -> 1;
              -> 2;
    endrecode;
    RecAge = x;
  end;

  { function to recode place of delivery it is used by several tables }
  function RecPDel1( chi )
    recode  M15(chi) -> x;
             20:29   -> 2;        { public health facility }
             30:39   -> 3;        { private health facility }
             40:49   -> 4;        { private NGO facility }
            missing  -> missing;
			         -> 5;        { elsewhere }
    endrecode;
    RecPDel1 = x;
  end;

  { function to recode place of delivery it is used by several tables }
  function RecPDel2( chi )
    recode  M15(chi) -> x;
             20:49   -> 1;        { health facility }
           missing   -> 9;        { missing }
                     -> 2;        { elsewehere  }
    endrecode;
    RecPDel2 = x;
  end;

  { function to recode antenatal care visits it is used by several tables }
  function RecAntCare( chi )
    recode  M14(chi) -> x;
            notappl  -> notappl;
                0    -> 0;
              1:3    -> 1;
             4:90    -> 2;
                     -> 9;
    endrecode;
    RecAntCare = x;
  end;

  { facilitates tabulation of table 9.5 as every cell has to be tallied individually }
  function tabt905( category )
    if category = 1 then
	  if M80 = 1 then            // live births
        poutcome = 1;
        xtab( t905, rweight );
      endif;
	  if M80 = 3 then            // stillbirths
        poutcome = 2;
        xtab( t905, rweight );
      endif;
	  if livestil then          // only last birth/stillbirth
        poutcome = 3;
        xtab( t905, rweight );
      endif;
    endif;
  end;

PROC RECODE8_FF
preproc

  {+EN} SetLanguage("EN"); {EN+}   // !!!change to language to be used from valuesets in working dictionary
  {{FR} SetLanguage("FR"); {FR}}
  {{ES} SetLanguage("ES"); {ES}}

  total    = 0;
  totalt   = 0;
  percent  = 0;
  numwom   = 1;
  numchild = 1;
  numbirth = 1;
  numbist  = 1;
  numhh    = 1;
  totnum2  = 1;
  totnum7  = 2;
  totbirth = 1;
  totbirt2 = 2;
  mednum   = 0;
  maxvist  = 10;
  livbirth = 1;
  stlbirth = 1;
  livestil = 1;

  unweight = ( sysparm()[1:1] = "U" );   { 0-Weighted, 1-unweighted }

postproc

  { constructs table to determine whether run is weighted/unweighted }
  txxx(unweight,0) = sysdate( "dd" );      { day   }
  txxx(unweight,1) = sysdate( "mm" );      { month }
  txxx(unweight,2) = sysdate( "yyyy" );    { year  }

  { Table 9.1 processing }
  jtot = tblcol( t901 );
  jmax = jtot - 3;
  do j = 0 while j <= jmax
    t901[*,j,*] = t901[*,j,*] * 100 / t901[*,jtot,*];
  enddo;
  t901[*,jmax+1,*]  = tblsum( column t901[*,0:jmax,*] );
  { !! ANC by skilled provider, include first three categories }
  t901[*,jmax+2,*] = tblsum( column t901[*,0:2,*] );
  { check unweighted N's }
  Col3Dim( "t901", t901, 0,      jtot-3, t901u, 0 );
  Col3Dim( "t901", t901, jtot-1, jtot-1, t901u, 0 );
  { assign defaults for layers stillbirths and total births/stillbirths }
  itot = tblrow( t901 );
  t901[0:itot-1,*,1]  = default;    // stillbirths
  t901[0:itot-1,*,2]  = default;    // live births and stillbirths
  t901u[0:itot-1,*,1] = default;    // stillbirths 
  t901u[0:itot-1,*,2] = default;    // live births and stillbirths 

  { Table 9.2 processing }
  jtot = tblcol( t902, numwom = 1 );
  jmax = jtot - 2;
  do j = 0 while j <= jmax
    t902[*,j,*] = t902[*,j,*] * 100 / t902[*,jtot,*];
  enddo;
  jmax1            = tblcol( t902, m14w1 ) + 1;    // use instead of total(1) because doesn't compile
  t902[*,jmax1,*]  = tblsum( column t902[*,0:jmax1-1,*] );
  jmax2            = tblcol( t902, m13w ) + 1;     // use instead of total(1) because doesn't compile
  t902[*,jmax2,*]  = tblsum( column t902[*,jmax1+2:jmax2-1,*] );
  jmax             = tblcol( t902, mednum = 0 );
  t902[*,jmax,*]   = tblmed( column t902w[*,0:maxvist,*]  intervals(highest default) );
  { check unweighted N's }
  Col3Dim( "t902", t902, 0,       jmax1-1, t902u, 0 );
  Col3Dim( "t902", t902, jmax1+1, jtot-2,  t902u, 0 );
  Col3Dim( "t902", t902, jtot+1,  jtot+1,  t902u, 1 );
  { assign defaults for layers stillbirths and total births/stillbirths }
  itot = tblrow( t902 );
  t902[0:itot-1,*,1]  = default;    // stillbirths
  t902[0:itot-1,*,2]  = default;    // live births and stillbirths
  t902u[0:itot-1,*,1] = default;    // stillbirths
