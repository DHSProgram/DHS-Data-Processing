    recode CHD30 -> cervix;
             0   -> 2;
             1   -> 1;
                 -> CHD30;
    endrecode;				 
    xtab( tcd09, rweight );
    coltotu = 1;     
    xtab( tcd09u );
    wtestp   = notappl;
    cervix   = notappl;
