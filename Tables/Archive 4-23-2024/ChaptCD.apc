      xtab( tcd09, rweight );
      coltotu = 2;     
      xtab( tcd09u );
      wtreat1  = notappl;
      if CHD31 = 1 then	               // received treatment the same day
        treatch = 1;	
        xtab( tcd09, rweight );
      endif;		
      if CHD32 = 1 then	               // had a follow-up visit
        treatch = 2;	
        xtab( tcd09, rweight );
      endif;		
      treatch  = notappl;
    endif;		
  endif;	
  if CHD29 = 4 | CHD30 in 0,8 then	   // test unclear/inconclusive, didn't receive results/DK
    wtreat2 = 1;	
    visitfol = ( CHD33 = 1 );	  
    xtab( tcd09, rweight );
    coltotu = 3;     
    xtab( tcd09u );
    wtreat2  = notappl;
    visitfol = notappl;	
  endif;		
