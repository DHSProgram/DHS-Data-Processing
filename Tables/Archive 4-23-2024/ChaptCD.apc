  if CHD27 = 1 then            // have been tested for cervical cancer
    numwom = 1;
    recode CHD28 -> timetest;  // time of last cervical cancer test
             0   -> 1;
            1:3  -> 2;
			4:40 -> 3;
			 98  -> 8;
                 -> default;
    endrecode;
    recode CHD29 -> restest;  // cervical cancer test results
            2,3  -> 1;
             1   -> 2;
             4   -> 3;
             5   -> 4;
                 -> CHD29;
    endrecode;				 
    xtab( tcd08, rweight );
    coltotu = 2;     
    xtab( tcd08u );
  endif;	
  
