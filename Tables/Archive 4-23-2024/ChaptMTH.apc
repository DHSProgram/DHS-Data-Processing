  recode MTH22 -> phqscore;	  
          0:4  -> 1;
          5:9  -> 2;
         10:14 -> 3;
         15:19 -> 4;
         20:27 -> 5;
               -> default;
  endrecode;	
  pctdep = ( MTH22 >= 10 );  
  xtab( tmth041, rweight );
  xtab( tmth041u );

{ --------------------------------------------------------------------------- }
{ Table MTH.5.1 }

  recode MTH24 -> gad2;	  
          0:5  -> 1;
          6:21 -> 2;
               -> default;
  endrecode;	
  recode MTH22 -> phq2;	  
          0:9  -> 1;
         10:27 -> 2;
               -> default;
  endrecode;	
  anxdepw2 = notappl;
  anxdepw1 = 4;  
  xtab( tmth051, rweight );
  coltotu = 1;
  xtab( tmth051u );
  if MTH20 = 1 then           // Ever told had anxiety
    anxdepw1 = 1;  
    xtab( tmth051, rweight );
  endif;	
  if MTH19 = 1 then           // Ever told had depression 
    anxdepw1 = 2;  
    xtab( tmth051, rweight );
  endif;	
  if MTH21 = 1 then           // In past 2 weeks took medicine for depression or anxiety
    anxdepw1 = 3;  
    xtab( tmth051, rweight );
  endif;	
  anxdepw1 = notappl;
  { respondents with symptoms of anxiety or depression }
  if MTH24 >= 1 | MTH22 >= 1 then  
    anxdepw2 = 2;  
    xtab( tmth051, rweight );
    coltotu = 2;
    xtab( tmth051u );
    if MTH17 = 1 then	      // Ever sought help
      anxdepw2 = 1;  
      xtab( tmth051, rweight );
    endif;
  endif;	

