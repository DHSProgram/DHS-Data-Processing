  freqdis = RecDis( MTH2 ); 
  xtab( tmth01, rweight );
  xtab( tmth01u );
  anxiety = 3;              // Worrying too much about different things
  freqdis = RecDis( MTH3 ); 
  xtab( tmth01, rweight );
  xtab( tmth01u );
  anxiety = 4;              // Trouble relaxing
  freqdis = RecDis( MTH4 ); 
  xtab( tmth01, rweight );
  xtab( tmth01u );
  anxiety = 5;              // Being so restless that it is hard to sit still
  freqdis = RecDis( MTH5 ); 
  xtab( tmth01, rweight );
  xtab( tmth01u );
  anxiety = 6;              // Becoming easily annoyed or irritable
  freqdis = RecDis( MTH6 ); 
  xtab( tmth01, rweight );
  xtab( tmth01u );
  anxiety = 7;              // Feeling afraid as if something awful might happen
