    t1501[*,j,*] = t1501[*,j,*] * 100 / t1501[*,jtot2,*];
  enddo;
  t1501[*,jmax+1,*] = tblsum( column t1501[*,jtot1+1:jmax,*] );
  { check unweighted N's }
  Col3Dim( "t1501", t1501, 0,       jtot1-1, t1501u, 0 );
  Col3Dim( "t1501", t1501, jtot1+1, jtot2-2, t1501u, 1 );
  { assign default to not applicable rows }
