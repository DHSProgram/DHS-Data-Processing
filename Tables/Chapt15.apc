  { function to recode decision making for man }
  function recdecis( varx )
    recode   varx -> temp;
               1  -> 4;     { respondent which is husband }
               2  -> 2;     { both equally }
               4  -> 1;     { wife }
               5  -> 5;     { someone else }
        8,missing -> 9;     { missing }
                  -> 6;  { other }
    endrecode;
    recdecis = temp;
  end;

PROC RECODE8_FF
preproc

  {+EN} SetLanguage("EN"); {EN+}   // !!!change to language to be used from valuesets in working dictionary
